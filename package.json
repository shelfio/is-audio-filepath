{
  "name": "@shelf/is-audio-filepath",
  "version": "2.0.0",
  "description": "Determine whether filepath is for audio file",
  "keywords": [
    "is-audio"
  ],
  "license": "MIT",
  "author": {
    "name": "Vitalii Sikora",
    "email": "vitalii@shelf.io",
    "url": "shelf.io"
  },
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib/ && yarn build:types && yarn build:code",
    "build:code": "babel src --out-dir lib --ignore '**/*.test.ts' --extensions '.ts' && find ./lib -name '*.test.d.ts' -delete",
    "build:types": "tsc --emitDeclarationOnly --declaration --isolatedModules false --declarationDir lib",
    "coverage": "yarn test --coverage",
    "lint": "yarn lint:ci --fix",
    "lint:ci": "eslint . --ext .js,.ts,.json",
    "prepack": "yarn build",
    "test": "TZ=UTC jest src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "lint-staged": {
    "*.{html,md,yml}": [
      "prettier --write"
    ],
    "*.{js,ts,json}": [
      "eslint --fix"
    ]
  },
  "babel": {
    "extends": "@shelf/babel-config/backend"
  },
  "prettier": "@shelf/prettier-config",
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.2",
    "@shelf/babel-config": "1.0.2",
    "@shelf/eslint-config": "2.18.0",
    "@shelf/prettier-config": "1.0.0",
    "@shelf/tsconfig": "0.0.6",
    "@types/jest": "28.1.8",
    "@types/node": "16",
    "eslint": "8.43.0",
    "husky": "8.0.2",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.8.8",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
